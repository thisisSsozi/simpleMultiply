{
    "extends": ["airbnb", "prettier","plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit":"off",
        "prefer-destructuring": "off",
        "node/no-unpublished-require": "off",
        "no-undef": "warn"
    }
}
